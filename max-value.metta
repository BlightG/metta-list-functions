(: list Type)
(: () list)
(: :: (-> Number list list))

(: max-value (-> list Number))
(= (max-value ()) ())
(= (max-value (:: $head $tail)) 
	(is-max $head $tail)
)

(: is-max (-> Number list Number))
(= (is-max $x ()) $x)
(= (is-max $x (:: $head $tail))
	(if (> $x $head)
		(is-max $x $tail)
		(is-max $head $tail)
	)
)

!(max-value (:: 10 (:: 20 (:: 15 ()))))
